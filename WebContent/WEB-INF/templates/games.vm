<html>
#parse("subs/header.vm")
<body>
<div class="container-fluid">
	#parse("subs/menu.vm")
	<div class="row">
		<div class="col-sm-12">
			<h1>Spill</h1>
		</div>
    	<div class="col-md-12 col-sm-12">
    		<div class="table-responsive">
 				<table class="table">
        			<tr>
            			<th>#</th><th>Navn</th><th>Antall spilt</th><th></th>
        			</tr>
        			#foreach( $game in $games )
            			<tr>
                			<td>$game.getId()</td>
                			<td>$game.getNavn()</td>
                			<td>$game.getSpillinger()</td>
                			<td><a href="/BoardgamesRegister/spill/spill/$game.getId()">Gå til</a></td>
            			</tr>
        			#end
    			</table>
    		</div>
    	</div>
    </div>
</div>

#parse("subs/bottom.vm")
